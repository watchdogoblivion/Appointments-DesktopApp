<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="anchor" prefHeight="320.0" prefWidth="300.0" stylesheets="@../styles/Login.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="samuel_dorilas.controllers.LoginFXMLController">
    <children>
      <Button fx:id="login" layoutX="46.0" layoutY="245.0" mnemonicParsing="false" onAction="#login" prefHeight="25.0" prefWidth="209.0" stylesheets="@../styles/Login.css" text="Login" textFill="WHITE">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <TextField fx:id="userNameField" layoutX="46.0" layoutY="135.0" prefHeight="25.0" prefWidth="209.0" />
      <PasswordField fx:id="passwordField" layoutX="46.0" layoutY="191.0" onAction="#login" prefHeight="25.0" prefWidth="209.0" />
      <Hyperlink fx:id="forgot" layoutX="40.0" layoutY="283.0" prefHeight="23.0" prefWidth="240.0" text="Forgot Username or Password?" textFill="WHITE">
         <font>
            <Font size="10.0" />
         </font>
      </Hyperlink>
      <ImageView fitHeight="107.0" fitWidth="154.0" layoutX="127.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/Logo3.png" />
         </image>
      </ImageView>
    </children>
</AnchorPane>
